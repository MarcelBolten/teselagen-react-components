    "react-measure": "^1.4.7",
    "react-redux": "^5.0.4",
    "redux-form": "^6.7.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.17.1",
    "@blueprintjs/datetime": "^1.15.0",
    "@blueprintjs/table": "^1.14.0",
    "babel-eslint": "7.1.1",
    "enzyme": "^2.8.2",
