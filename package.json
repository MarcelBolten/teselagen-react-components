{
  "name": "teselagen-react-components",
  "version": "4.0.9",
  "description": "Teselagen React Component Library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3333",
    "//test": "nwb test-react",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "//test:coverage": "nwb test-react --coverage",
    "//test:watch": "nwb test-react --server",
    "release": "release-it",
    "test": "jest src --coverage",
    "test:staged": "cross-env CI=true jest --env=jsdom --findRelatedTests",
    "test-watch": "jest src --watch",
    "test-debug": "node --inspect --debug-brk ./node_modules/.bin/jest src --runInBand "
  },
  "dependencies": {
    "basiccontext": "^3.5.1",
    "bio-parsers": "^4.2.0",
    "clipboard": "^1.7.1",
    "combokeys": "^2.4.6",
    "cross-env": "^5.0.1",
    "deep-equal": "^1.0.1",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.4",
    "lodash.flatmap": "^4.5.0",
    "lru-memoize": "^1.0.2",
    "paths-js": "^0.4.5",
    "pluralize": "^5.0.0",
    "randomcolor": "^0.5.0",
    "rc-slider": "^8.0.0",
    "react-addons-pure-render-mixin": "^15.5.2",
    "react-draggable": "^2.2.3",
    "react-dropzone": "^3.13.2",
    "react-icons": "^2.2.3",
    "react-list": "^0.8.5",
    "react-measure": "^1.4.7",
    "react-mouse-aware": "^0.3.0",
    "react-popover": "^0.4.14",
    "recompose": "^0.23.1",
    "redux": "^3.6.0",
    "redux-act": "^1.2.0",
    "reselect": "^3.0.0",
    "teselagen-interval-tree": "1.1.1",
    "ve-range-utils": "^2.0.37",
    "ve-sequence-utils": "3.1.17"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^1.18.0",
    "@blueprintjs/datetime": "^1.16.0",
    "@blueprintjs/table": "^1.15.0",
    "react-addons-css-transition-group": "^15.5.2",
    "react-redux": "^5.0.4",
    "react-select": "^1.0.0-rc.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.18.0",
    "@blueprintjs/datetime": "^1.16.0",
    "@blueprintjs/table": "^1.15.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-config-teselagen": "^1.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.13.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "lint-staged": "^3.5.1",
    "nwb": "0.15.x",
    "prettier": "^1.3.1",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.5",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "release-it": "^2.7.3"
  },
  "lint-staged": {
    "src/**/*.js ": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "yarn test:staged",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/es/"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/teselagen-react-components",
  "keywords": [
    "react-component",
    "teselagen"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
