{
  "name": "teselagen-react-components",
  "version": "1.0.32",
  "description": "Teselagen React Component Library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3333",
    "//test": "nwb test-react",
    "precommit": "lint-staged",
    "//test:coverage": "nwb test-react --coverage",
    "//test:watch": "nwb test-react --server",
    "release": "npm run build; release-it",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-debug": "node --inspect --debug-brk ./node_modules/.bin/jest --runInBand "
  },
  "dependencies": {
    "fuse-box": "^2.0.0",
    "pluralize": "^5.0.0"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^1.17.1",
    "@blueprintjs/datetime": "^1.15.0",
    "@blueprintjs/table": "^1.14.0",
    "react-addons-css-transition-group": "^15.5.2",
    "redux": "^3.6.0",
    "react-measure": "^1.4.7",
    "react-redux": "^5.0.4",
    "redux-form": "^6.7.0"
  },
  "devDependencies": {
    "lint-staged": "^3.4.1",
    "@blueprintjs/core": "^1.17.1",
    "prettier": "^1.3.1",
    "redux": "^3.6.0",
    "@blueprintjs/datetime": "^1.15.0",
    "@blueprintjs/table": "^1.14.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-config-teselagen": "^1.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "nwb": "0.15.x",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-measure": "^1.4.7",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "redux-form": "^6.7.0",
    "release-it": "^2.7.3"
  },
  "lint-staged": {
    "src/**/*.js ": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/teselagen-react-components",
  "keywords": [
    "react-component",
    "teselagen"
  ]
}
