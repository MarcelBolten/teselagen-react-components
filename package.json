{
  "name": "teselagen-react-components",
  "version": "19.1.26",
  "description": "Teselagen React Component Library",
  "main": "lib/index.js",
  "//module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "build-watch": "nodemon  --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3333",
    "precommit": "lint-staged",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "test": "jest --env=jsdom --maxWorkers 2",
    "test-coverage": "yarn test -- --coverage",
    "test:staged": "cross-env CI=true yarn test -- --findRelatedTests",
    "test-fix": "git reset . && cross-env CI=true yarn test --onlyChanged --updateSnapshot",
    "test-watch": "yarn test -- --watch",
    "test-debug": "node --inspect --debug-brk ./node_modules/.bin/jest src --runInBand "
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-env": "^5.1.1",
    "deep-equal": "^1.0.1",
    "dom-scroll-into-view": "^1.2.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "file-saver": "^1.3.3",
    "json.sortify": "^2.2.2",
    "lodash": "^4.17.4",
    "lodash.flatmap": "^4.5.0",
    "lru-memoize": "^1.0.2",
    "math-expression-evaluator": "^1.2.17",
    "nodemon": "^1.12.1",
    "papaparse": "^4.3.6",
    "pascal-case": "^2.0.1",
    "paths-js": "^0.4.5",
    "pluralize": "^7.0.0",
    "query-string": "^5.0.1",
    "react-addons-pure-render-mixin": "^15.6.0",
    "react-dropzone": "^4.2.1",
    "react-sortable-hoc": "^0.6.8",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "uniqid": "^4.1.1",
    "ve-range-utils": "^2.3.3"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^1.18.0",
    "@blueprintjs/datetime": "^1.16.0",
    "@blueprintjs/labs": "^0.14.1",
    "@blueprintjs/table": "^1.15.0",
    "apollo-cache-invalidation": "^0.0.3",
    "react-addons-css-transition-group": "^15.5.2",
    "react-apollo": "^1.4.15",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "react-table": "^6.5.3",
    "redux-form": "^6.7.0",
    "tg-client-query-builder": "^2.6.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.35.0",
    "@blueprintjs/datetime": "^1.25.1",
    "@blueprintjs/labs": "^0.14.1",
    "@blueprintjs/table": "^1.31.0",
    "apollo-cache-invalidation": "^0.0.3",
    "axios": "^0.17.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chance": "^1.0.12",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-teselagen": "^4.0.9",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "in-publish": "^2.0.0",
    "jest": "^21.2.1",
    "lint-staged": "^4.1.0",
    "nwb": "^0.19.0",
    "prettier": "^1.6.1",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.0",
    "react-apollo": "^1.4.15",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.5",
    "react-table": "^6.7.4",
    "react-test-renderer": "^16.0.0",
    "redux-form": "^7.1.2",
    "redux-thunk": "*",
    "sinon": "^4.0.2",
    "tap": "^10.7.2",
    "tg-client-query-builder": "^2.6.0"
  },
  "lint-staged": {
    "src/**/*.js ": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/es/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/teselagen-react-components",
  "keywords": [
    "react-component",
    "teselagen"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
