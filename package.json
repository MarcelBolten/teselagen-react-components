    "react-dom": "^15.5.4",
    "react-measure": "^1.4.7",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux-form": "^6.7.0",
    "release-it": "^2.7.3"
  },
  "lint-staged": {
    "src/**/*.js ": [
      "prettier --write",
